<h2>ジャンル一覧・追加</h2>
<p>ジャンル名</p>
    <%= form_with model: @genre,url: admin_genres_path, local:true do |f| %>
    <%= f.text_field :name %>
    <%= f.submit '新規登録' %>
    <% end %>

<p>ジャンル</p>
<% @genres.each do |genre| %>
    <p>ジャンル</p>
    <p><%= genre.name %></p>
    <p><%= link_to '編集する', edit_admin_genre_path(genre.id) %></p>
<% end %>